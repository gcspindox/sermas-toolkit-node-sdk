{
  "name": "@sermas/sermas-toolkit-node-sdk",
  "version": "1.0.0-alpha.93",
  "description": "Sermas toolkit node SDK",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sermas-eu/sermas-toolkit-node-sdk.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsup && cp package.json README.md LICENSE dist/",
    "build:watch": "tsup --watch",
    "start": "node index.js",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@sermas/api-client": "^1.0.0-alpha.162",
    "axios": "^1.7.1",
    "eventemitter2": "^6.4.9",
    "openai": "^4.47.2",
    "ulidx": "^2.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.2.5",
    "tsup": "^8.1.0",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
