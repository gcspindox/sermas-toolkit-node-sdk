{
  "name": "@sermas/sermas-toolkit-node-sdk",
  "version": "1.0.0-alpha.8",
  "description": "Sermas toolkit node SDK",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sermas-eu/sermas-toolkit-node-sdk.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && tsc -p tsconfig.types.json",
    "build:watch": "concurrently \"npx tsc -p tsconfig.cjs.json --watch\" \"npx tsc -p tsconfig.types.json --watch\"",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "dependencies": {
    "@sermas/api-client": "^1.0.0-alpha.54",
    "axios": "^1.7.1",
    "eventemitter2": "^6.4.9",
    "openai": "^4.47.2",
    "ulidx": "^2.3.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.2.5",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0"
}
